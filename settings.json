{
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "antfu.slidev"
  },
  "breadcrumbs.enabled": false,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.cpp.disabledLanguages": [],
  // ============================================================================
  // Extension Settings
  // ============================================================================
  // Make the UI cleaner and more minimal by hiding or refining various elements
  "custom-ui-style.font.monospace": "Fira Code",
  "custom-ui-style.stylesheet": {
    ".explorer-folders-view": "font-family: Fira Code; font-size: 11px;",
    ".quick-input-widget": "top: 50% !important; left: 50% !important; transform: translate(-50%, -50%);",
    ".tabs-container": "font-family: Fira Code;",
    "kbd, .statusbar": {
      "font-family": "var(--cus-monospace-font)"
    }
  },
  "diffEditor.renderSideBySide": false,
  "editor.cursorSurroundingLines": 10,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 11,
  "editor.formatOnSave": true,
  // ============================================================================
  // Editor Settings
  // ============================================================================
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.tsx": "${capture}.ts, ${capture}.test.tsx, ${capture}.stories.tsx, ${capture}.fixtures.ts, ${capture}.store.ts"
  },
  "files.exclude": {
    "**/.git": false
  },
  "git.replaceTagsWhenPull": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  // This is a workaround to ensure the terminal is closed after LazyGit is used
  "lazygit.command": "bash -c lazygit && exit",
  // Sends vscode keybindings to the integrated terminal to allow for vim-like keybindings
  "terminal.integrated.sendKeybindingsToShell": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  // ============================================================================
  // User-Defined Settings
  // ============================================================================
  "update.releaseTrack": "prerelease",
  "vim.highlightedyank.color": "rgba(250, 189, 47, 0.5)",
  "vim.highlightedyank.duration": 150,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "#202020",
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      // Triggers the whichkey popup using the space key as the leader key
      // The space key acts as the leader for custom shortcuts
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    },
    {
      "before": ["]", "t"],
      "commands": ["workbench.action.nextGroup"]
    },
    {
      "before": ["[", "t"],
      "commands": ["workbench.action.previousGroup"]
    },
    {
      "before": ["\\"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["|"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["<", "b"],
      "commands": ["workbench.action.moveEditorLeftInGroup"]
    },
    {
      "before": [">", "b"],
      "commands": ["workbench.action.moveEditorRightInGroup"]
    },
    {
      // Insert comment below current line
      "before": ["g", "c", "o"],
      "commands": ["cursorDown", "editor.action.addCommentLine"]
    },
    {
      // Insert comment above current line
      "before": ["g", "c", "O"],
      "commands": ["cursorUp", "editor.action.addCommentLine"]
    },
    {
      "before": ["]", "q"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "q"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "Q"],
      "commands": ["editor.action.marker.last"]
    },
    {
      "before": ["[", "Q"],
      "commands": ["editor.action.marker.first"]
    },
    {
      "before": ["]", "l"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "l"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["]", "L"],
      "commands": ["editor.action.marker.lastInFiles"]
    },
    {
      "before": ["[", "L"],
      "commands": ["editor.action.marker.firstInFiles"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      // Show line diagnostics (problems) for current line
      "before": ["g", "l"],
      "commands": ["editor.action.showHover"]
    },
    {
      // Code Actions
      "before": ["g", "r", "a"],
      "commands": ["editor.action.codeAction"]
    },
    {
      // Rename symbol (grn)
      "before": ["g", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      // Diagnostic Next
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      // Diagnostic Previous
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      // Diagnostic Error Next
      "before": ["]", "e"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      // Diagnostic Error Previous
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      // Diagnostic Warning Next
      "before": ["]", "w"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      // Diagnostic Warning Previous
      "before": ["[", "w"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      // Document Symbol Next
      "before": ["]", "y"],
      "commands": ["editor.action.goToNextSymbolFromResult"]
    },
    {
      // Document Symbol Previous
      "before": ["[", "y"],
      "commands": ["editor.action.goToPreviousSymbolFromResult"]
    },
    {
      // Document Symbol
      "before": ["g", "O"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      // Declaration
      "before": ["g", "D"],
      "commands": ["editor.action.revealDeclaration"]
    },
    {
      // Type Definition
      "before": ["g", "y"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      // Definition
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      // Implementation
      "before": ["g", "r", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      // References
      "before": ["g", "r", "r"],
      "commands": ["editor.action.goToReferences"]
    }
  ],
